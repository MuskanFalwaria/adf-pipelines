{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-muskan-capg"
		},
		"ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2'"
		},
		"ADLSSink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSSink'"
		},
		"AdvWorksDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlCustomerDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlCustomerDatabase'"
		},
		"BlobStorageProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageProducts'"
		},
		"TaxiTripDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'TaxiTripDb'"
		},
		"TripAggregateBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TripAggregateBlob'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sqlstorage9828.dfs.core.windows.net/"
		},
		"ADLSSink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlstripdata9828.dfs.core.windows.net/"
		},
		"AdvWorksDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-muskan.database.windows.net"
		},
		"AdvWorksDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "muskan-db"
		},
		"AdvWorksDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "muskan"
		},
		"AzureSqlCustomerDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure-sql-server.database.windows.net"
		},
		"AzureSqlCustomerDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "muskan-sql-db"
		},
		"AzureSqlCustomerDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "muskan"
		},
		"TaxiTripDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-muskan.database.windows.net"
		},
		"TaxiTripDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "taxi-data"
		},
		"TaxiTripDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "muskan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-muskan-capg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_accountKey": {
						"value": "[parameters('ADLSGen2_accountKey')]"
					},
					"ADLSSink_accountKey": {
						"value": "[parameters('ADLSSink_accountKey')]"
					},
					"AdvWorksDb_password": {
						"value": "[parameters('AdvWorksDb_password')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlCustomerDatabase_password": {
						"value": "[parameters('AzureSqlCustomerDatabase_password')]"
					},
					"BlobStorageProducts_connectionString": {
						"value": "[parameters('BlobStorageProducts_connectionString')]"
					},
					"TaxiTripDb_password": {
						"value": "[parameters('TaxiTripDb_password')]"
					},
					"TripAggregateBlob_connectionString": {
						"value": "[parameters('TripAggregateBlob_connectionString')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"ADLSSink_properties_typeProperties_url": {
						"value": "[parameters('ADLSSink_properties_typeProperties_url')]"
					},
					"AdvWorksDb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_server')]"
					},
					"AdvWorksDb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_database')]"
					},
					"AdvWorksDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_userName')]"
					},
					"AzureSqlCustomerDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlCustomerDatabase_properties_typeProperties_server')]"
					},
					"AzureSqlCustomerDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlCustomerDatabase_properties_typeProperties_database')]"
					},
					"AzureSqlCustomerDatabase_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlCustomerDatabase_properties_typeProperties_userName')]"
					},
					"TaxiTripDb_properties_typeProperties_server": {
						"value": "[parameters('TaxiTripDb_properties_typeProperties_server')]"
					},
					"TaxiTripDb_properties_typeProperties_database": {
						"value": "[parameters('TaxiTripDb_properties_typeProperties_database')]"
					},
					"TaxiTripDb_properties_typeProperties_userName": {
						"value": "[parameters('TaxiTripDb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-muskan-capg_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-muskan-capg_ArmTemplate_0')]"
			]
		}
	]
}