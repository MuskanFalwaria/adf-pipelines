{
	"name": "TransformMovies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesDB",
						"type": "DatasetReference"
					},
					"name": "MoviesDB"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FilterYears"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesDB",
				"MoviesDB filter(toInteger(year) >= 1910 && toInteger(year) <= 2000) ~> FilterYears"
			]
		}
	}
}